<html>

    <head>
        <title></title>
    </head>

    <body>
        <h1>Atualizar Feedback</h1>

        <form action="salvarFeedback" method="post">

            <input type="hidden" name="ID" id="ID"  value="<%= (feedback.length > 0 ? feedback[0].ID: '') %>">

            <br>

            <label for="NOME">Nome:</label>
            <input type="text" name="NOME" id="NOME" value="<%= (feedback.length > 0 ? feedback[0].NOME:'') %>"  size="15"><br>

            <br>

            <label for="EMAIL">Email:</label>
            <input type="text" name="EMAIL" id="EMAIL" value="<%= (feedback.length > 0 ? feedback[0].EMAIL:'') %>" size="15"><br>
            
            <br>

            <label for="MENSAGEM">Mensagem:</label>
            <input type="text" name="MENSAGEM" id="MENSAGEM" value="<%= (feedback.length > 0 ? feedback[0].MENSAGEM:'') %>" size="15"><br>
            
            <br>

            <input type="submit" name="acao" value="Atualizar">
            <input type="submit" name="acao" value="Limpar">

        </form>

    </body>


</html>